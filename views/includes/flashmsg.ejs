<% if(success && success.length){ %>
  <div  class="mt-[41px]  alert alert-success alert-dismissible fade show col-6  offset-3 my-2" role="alert">
    <%= success %>
      <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"> </button>
  </div> 
  <% } %>
    <% if(error && error.length){ %>
      <div class="alert mt-[41px]  alert-danger alert-dismissible fade show col-6  offset-3 my-2" role="alert">
        <%= error %>
          <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"> </button>
      </div>
      <% } %>
        <% if( delet && delet.length){ %>
          <div class="alert mt-[41px]  alert-danger alert-dismissible fade show col-6  offset-3 my-2" role="alert">
            <%= delet %>
              <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"> </button>
          </div>
          <% } %>